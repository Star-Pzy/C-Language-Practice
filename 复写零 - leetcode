void func(int* arr,int i,int arrSize)
{
    int j = 0;
    for(j=arrSize-1;j>i;j--)
    {
        arr[j]=arr[j-1];
    }
}

void duplicateZeros(int* arr, int arrSize) {
    int i = 0;
    int j = 0;
    int tmp = 0;
    for(i=0;i<arrSize;i++)
    {
        if(arr[i]==0)
        {
            if(i!=arrSize-1)
            {
                func(arr,i,arrSize);
                i++;
            }
        }
    }
}
